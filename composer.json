{
    "name": "jfcherng/php-ip-location",
    "description": "利用 IP 查詢地理位置。",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jack Cherng",
            "email": "jfcherng@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jfcherng\\IpLocation\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phan/phan": "^1.0",
        "phpunit/phpunit": "^7.2"
    },
    "scripts": {
        "analyze": [
            "phan --color"
        ],
        "fix": [
            "php-cs-fixer fix --verbose"
        ],
        "test": [
            "phpunit --verbose"
        ]
    }
}
