{
    "name": "jfcherng/php-ip-location",
    "description": "利用 IP 查詢地理位置。",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jack Cherng",
            "email": "jfcherng@gmail.com"
        }
    ],
    "minimum-stability": "beta",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Jfcherng\\IpLocation\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "ipip/db": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phan/phan": "^2 || ^3",
        "phpunit/phpunit": ">=7 <10",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "sort-packages": true
    },
    "scripts": {
        "analyze": [
            "phan --color",
            "phpcs --colors -n"
        ],
        "fix": [
            "php-cs-fixer fix --verbose"
        ],
        "test": [
            "phpunit --verbose"
        ]
    }
}
